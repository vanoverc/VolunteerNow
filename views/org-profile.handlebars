<head>
    <!-- Bootstrap Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    <link rel="stylesheet" type="text/css" href="/css/organization.css" />
    <link rel="stylesheet" type="text/css" href="/css/main.css" />
</head>

<div id="org-profile-container">
    <div class="container-fluid">
        <div class="row">
            <div class="col-xs-6 col-xs-offset-3 col-md-4 col-md-offset-4">
                <div id="org-profile-heading" class="jumbotron">
                    <h1>{{org.organization_name}}</h1>
                </div>
                <div id="org-profile-body" align="center">
                    <div>
                        <button id="edit-profile-btn" type="button" class="btn index-btn">Edit Profile</button>
                    </div>
                    <div>
                        <button id="view-postings-btn" type="button" class="btn index-btn">Postings</button>
                    </div>
                    <div>
                        <button id="create-event-link-btn" type="button" class="btn index-btn">Create Event</button>
                    </div>
                    <div>
                        <button type="button" class="btn index-btn">Messages</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- jQuery -->
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<!-- Latest compiled and minified JavaScript -->
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
<script>
    $("#edit-profile-btn").click(function () {
        window.location.href = "/org-profile-edit";
    });
    $("#view-postings-btn").click(function () {
        window.location.href = "/org-profile-postings";
    });
    $("#create-event-link-btn").click(function () {
        window.location.href = "/org-event-creation";
    });
</script>
